{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://contextsuite.com/schemas/base_event_info_schema.json",
  "title": "ContextSuite Semantic Event Base Event Info",
  "description": "Schema for information about a base event if the current event is derived.",
  "type": ["null", "object"],
  "properties": {
    "event_gid": { "type": ["null", "string"], "format": "uuid" },
    "type": { "type": ["null", "string"] },
    "event": { "type": ["null", "string"] },
    "timestamp": { "type": ["null", "string"], "format": "date-time" },
    "message_id": { "type": ["null", "string"] },
    "entity_gid": { "type": ["null", "string"], "format": "uuid" }
  },
  "additionalProperties": false
}
